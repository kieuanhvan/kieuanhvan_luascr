    gg.clearResults()  

    end

  function Ghost() 
            local flag = true
          while true do
          if gg.isVisible(true) then
          gg.clearResults()
          if flag then
            flag = false
            SetValue("libil2cpp.so",0x1fb70fc,"0000A0E31EFF2FE1")
            gg.clearResults()
              gg.setVisible(false)
              gg.toast("ON")
          elseif flag == false then
            flag = true
            SetValue("libil2cpp.so",0x1fb70fc,"F04F2DE91CB08DE2")
            gg.clearResults()
              checkDate()
              gg.setVisible(false)
              gg.toast("OFF")
              end
          end
          gg.sleep(200)
          end
          gg.clearResults()
    end
  
