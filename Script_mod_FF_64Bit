  function LOBBY()
    gg.alert(" ANTIBAN SCRIPT ")
    gg.toast(" KAV EXPENSIVE")
    print(" KAV EXPENSIVE")
    print("SHARE KAV EXPENSIVE")
    gg.skipRestoreState()
    gg.setVisible(true)
    os.exit()
  end
  
  while true do
    if gg.isVisible(true) then
      LuaLibraryTool = 1
      gg.setVisible(false)
    end
    if LuaLibraryTool == 1 then
      HOME()
    end
  end
  